  author = {Gelman, Andrew and Vehtari, Aki and Simpson, Daniel and Margossian, Charles C. and Carpenter, Bob and Yao, Yuling and Kennedy, Lauren and Gabry, Jonah and BÃ¼rkner, Paul-Christian and ModrÃ¡k, Martin},
  date = {2020-11-03},
