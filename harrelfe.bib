@article{luo16can,
  title = {Canonical Variate Regression},
  author = {Luo, Chongliang and Liu, Jin and Dey, Dipak K. and Chen, Kun},
  date = {2016-07},
  journaltitle = {Biostatistics},
  volume = {17},
