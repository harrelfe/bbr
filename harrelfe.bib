@article{ber87tesa,
  title = {Testing {{Precise Hypotheses}}},
  author = {Berger, James O. and Delampady, Mohan},
  date = {1987-08},
  journaltitle = {Statistical Science},
  volume = {2},
  number = {3},
  pages = {317--335},
  publisher = {{Institute of Mathematical Statistics}},
