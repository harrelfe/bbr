  author = {Khan, Muhammad Shahzeb and Fonarow, Gregg C. and Friede, Tim and Lateef, Noman and Khan, Safi U. and Anker, Stefan D. and Harrell, Frank E. and Butler, Javed},
  date = {2020-08-03},
  journaltitle = {JAMA Netw Open},
  volume = {3},
