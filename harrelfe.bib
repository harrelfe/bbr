@article{tan15met,
  title = {Meta-Analysis for {{Discovering Rare-Variant Associations}}: {{Statistical Methods}} and {{Software Programs}}},
  author = {Tang, Zheng-Zheng and Lin, Dan-Yu},
  date = {2015-07},
  journaltitle = {The Am J Hum Gen},
  volume = {97},
