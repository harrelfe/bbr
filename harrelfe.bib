@article{kar19opt,
  title = {Optimizing {{Sample Size Allocation}} and {{Power}} in a {{Bayesian Two-Stage Drop-the-Losers Design}}},
  author = {Karanevich, Alex and Meier, Richard and Graw, Stefan and McGlothlin, Anna and Gajewski, Byron},
