@article{luo21bay,
  title = {Bayesian Latent Multi-State Modeling for Nonequidistant Longitudinal Electronic Health Records},
  author = {Luo, Yu and Stephens, David A. and Verma, Aman and Buckeridge, David L.},
  date = {2021},
