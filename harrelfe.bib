  title = {Multivariate Dynamic Model for Ordinal Outcomes},
  author = {Chaubert, F. and Mortier, F. and Saint AndrÃ©, L.},
  date = {2008-09-01},
  journaltitle = {Journal of Multivariate Analysis},
  volume = {99},
